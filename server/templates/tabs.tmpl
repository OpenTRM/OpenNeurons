<ul class="nav nav-tabs" id="myTab">
    {%for tab in tabs %}
      <li><a data-target="#{{tab.id}}" data-toggle="tab">{{tab.name}}</a></li>
    {%endfor%}
</ul>

<div class="tab-content" style="width:600px">
    <div class="tab-pane active">
        Select the tab.
    </div>

    {%for tab in tabs %}
        <div class="tab-pane" id="{{tab.id}}" style="width:600px">
            <p>{{tab.tag}}&nbsp;{{tab.source}}</p>

            {%with head=tab.head, table=tab.table, template_name="table.tmpl" %}
                   {%include template_name%}
            {%endwith%}
        </div>
    {%endfor%}
</div>
